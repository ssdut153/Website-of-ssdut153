doctype html
html
    head
        // 页面标题
        title 大连理工大学软153级队 | 主页
        include includes/head.pug
        link(href='css/owl.carousel.css', rel='stylesheet')
        link(rel='stylesheet', href='css/slippry.css')
        // //Owl Carousel Assets
    body
        // start header
        include includes/header_bg.pug
        // start header
        include includes/header_btm.pug
        // -start-banner--
        #move-top.banner
            // --start-image-slider--
            #home.img-slider(data-scroll-reveal='enter bottom but wait 0.7s')
                .wrap
                    .slider
                        ul#jquery-demo
                            li
                                a(href='#slide1')
                                .slider-detils(data-scroll-reveal='enter bottom but wait 0.7s')
                                    // 大标题
                                    h3 软件 —— 追溯过去
                                    // 小标题
                                    span 再小的细节也能明察秋毫
                            li
                                a(href='#slide2')
                                .slider-detils(data-scroll-reveal='enter bottom but wait 1s')
                                    h3 软件 —— 着手现在
                                    span 每一个字符都意味着成功
                            li
                                a(href='#slide3')
                                .slider-detils(data-scroll-reveal='enter bottom but wait 1.5s')
                                    h3 软件 —— 放眼未来
                                    span 你的未来由你决定
            .clear
        // 版权 -
        .copyrights
            | Collect from
            a(href='http://www.cssmoban.com/', title='网站模板') 网站模板
        // -slider--
        // -scrooling-script-
        // --//End-image-slider--
        .simple-text
            .wrap
                // slider下主标题 -
                h4 大连理工大学软153级队
                // slider下副标题 -
                p 你最好的伙伴
        // 级队委 -
        include includes/index/recent_works.pug
        // 近期发表 -
        include includes/index/last_posts.pug
        // 声明
        .testimonial
            // start testimonial
            .wrap
                h5.heading 公告
                .test-grids
                    .test-desc
                        // 标题 -
                        h3 公告标题
                        // 内容 可多p段 -
                        p 本部分内容尚未确定
                    .img_1
                        // 图片 不建议修改 -
                        img(src='images/avator.png')
                    .clear
        // 联系我们
        include includes/index/get_in_touch.pug
        include includes/footer.pug
        include includes/copy.pug
        include includes/js.pug
        script(src='js/jquery-ui.js')
        script(src='js/scripts-f0e4e0c2.js')
        script.
            $('#jquery-demo').slippry({/*general elements & wrapper*/slippryWrapper: '<div class="sy-box jquery-demo" />', /* wrapper to wrap everything, including pager*//* options*/adaptiveHeight: true, /* height of the sliders adapts to current slide*/useCSS: true, /* true, false -> fallback to js if no browser support*/autoHover: false, transition: 'fade'});
        script(src='js/owl.carousel.js')
        script.
            $(document).ready(function () {$("#owl-demo").owlCarousel({items: 4, lazyLoad: true, autoPlay: true, navigation: true, navigationText: ["", ""], rewindNav: false, scrollPerPage: false, pagination: false, paginationNumbers: false});});
        script(src='js/index.js')
        script(src='js/nivo-lightbox.min.js')
        script.
            $(document).ready(function () {$('.nivo-lightbox-demo a').nivoLightbox({effect: 'fade'});});
        include includes/bd.pug